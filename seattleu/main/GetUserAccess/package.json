{
  "name": "downloadfrommedialibrary",
  "version": "1.0.1",
  "description": "",
  "main": "./dist/bundle.js",
  "bin": "./dist/bundle.js",
  "type": "commonjs",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "mkdir -p dist && esbuild index.mjs api.mjs --bundle --platform=node --outdir=dist --format=cjs  --external:node:* --external:fs --external:path",
    "package": "pkg . --no-bytecode --debug --public-packages '*' --public --targets latest-win-x64 --output dist/get-editor-info.exe"
  },
  "author": "Rohit Nagotkar",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^2.7.0",
    "t4.ts": "^0.0.11",
    "tslib": "^2.8.1",
    "xlsx": "^0.18.5",
    "zip-a-folder": "^3.1.6"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "promptUI/**/*",
      "t4apiwrapper/**/*",
      "dist/**/*",
      "api.mjs"
    ],
    "scripts": [
      "index.js",
      "../promptUI/**/*.mjs",
      "../promptUI/**/*.js",
      "../../../../t4apiwrapper/**/*.js",
      "!build/**/*.test.js",
      "api.mjs"

    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "pkg": "^5.8.1"
  }
}
